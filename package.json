{
  "name": "cycle-todomvc",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "author": "addhome2001",
  "scripts": {
    "start": "webpack-dev-server --profile --colors --progress",
    "build": "webpack --profile --colors --progress --config ./webpack.prod.config.js",
    "build:prod": "better-npm-run build:prod",
    "build:demo": "npm run clear:demo && better-npm-run build:demo",
    "clear:demo": "rimraf docs",
    "debug": "better-npm-run debug",
    "lint": "./node_modules/.bin/eslint .",
    "ava": "better-npm-run ava",
    "test:watch": "better-npm-run test:watch",
    "test": "better-npm-run test",
    "validate": "npm run lint && npm test"
  },
  "betterScripts": {
    "build:prod": {
      "command": "npm run build",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build:demo": {
      "command": "npm run build:prod",
      "env": {
        "DEMO": 1
      }
    },
    "debug": {
      "command": "npm start",
      "env": {
        "DEBUG": "*"
      }
    },
    "ava": {
      "command": "./node_modules/.bin/ava",
      "env": {
        "NODE_ENV": "test"
      }
    },
    "test": {
      "command": "nyc npm run ava"
    },
    "test:watch": {
      "command": "npm run ava -- -w --verbose"
    }
  },
  "engines": {
    "node": ">=6.2.2",
    "npm": ">=3.9.5"
  },
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "file-loader": "^0.11.1",
    "img-loader": "^2.0.0",
    "rimraf": "^2.6.1",
    "url-loader": "^0.5.8",
    "webpack-dev-server": "^2.4.2"
  },
  "dependencies": {
    "@cycle/collection": "^0.5.1",
    "@cycle/dom": "^17.1.0",
    "@cycle/http": "^13.2.0",
    "@cycle/rxjs-adapter": "^3.0.3",
    "@cycle/rxjs-run": "^7.0.0",
    "autoprefixer": "^6.7.7",
    "ava": "^0.19.1",
    "better-npm-run": "^0.0.15",
    "css-loader": "^0.28.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.1.3",
    "eslint-plugin-import": "^2.2.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^2.28.0",
    "nyc": "^10.2.0",
    "postcss-loader": "^1.3.3",
    "precss": "^1.4.0",
    "rxjs": "^5.3.0",
    "sinon": "^2.3.2",
    "style-loader": "^0.16.1",
    "webpack": "^2.4.1",
    "webpack-md5-hash": "^0.0.5"
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit",
    "files": [
      "test/**/*.js"
    ]
  }
}
