{
  "name": "cycle-todomvc",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "author": "addhome2001",
  "scripts": {
    "start": "webpack-dev-server --profile --colors --progress",
    "build": "webpack --profile --colors --progress --config ./webpack.prod.config.js",
    "build:prod": "cross-env NODE_ENV=production npm run build",
    "build:demo": "npm run clear:demo && cross-env NODE_ENV=production DEMO=1 npm run build",
    "clear:demo": "rm -rf docs",
    "debug": "cross-env DEBUG=* npm start",
    "lint": "./node_modules/.bin/eslint .",
    "test:watch": "cross-env NODE_ENV=test ./node_modules/.bin/ava -w --verbose",
    "test": "cross-env NODE_ENV=test nyc ./node_modules/.bin/ava",
    "validate": "npm run lint && npm test"
  },
  "engines": {
    "node": ">=6.2.2",
    "npm": ">=3.9.5"
  },
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "cross-env": "^3.1.4",
    "webpack-dev-server": "2.2.0"
  },
  "dependencies": {
    "@cycle/collection": "0.4.0",
    "@cycle/dom": "13.0.0",
    "@cycle/http": "11.2.0",
    "@cycle/rxjs-adapter": "3.0.3",
    "@cycle/rxjs-run": "^3.0.3",
    "autoprefixer": "6.5.1",
    "ava": "0.17.0",
    "css-loader": "0.25.0",
    "eslint": "3.12.0",
    "eslint-config-airbnb-base": "10.0.1",
    "eslint-plugin-import": "2.2.0",
    "extract-text-webpack-plugin": "beta",
    "html-webpack-plugin": "^2.28.0",
    "nyc": "10.1.2",
    "postcss-loader": "1.0.0",
    "precss": "1.4.0",
    "rxjs": "5.0.3",
    "style-loader": "0.13.1",
    "webpack": "2.2.0",
    "webpack-md5-hash": "^0.0.5"
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit",
    "files": [
      "test/**/*.js"
    ]
  }
}
